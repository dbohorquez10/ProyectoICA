	<parameter name="P_ID_PRODUCTOR" class="java.lang.String"/>
	<parameter name="LOGO_ICA" class="java.lang.String"/>
	<query language="sql"><![CDATA[
SELECT
    p.ID_PRODUCTOR,
    p.NOMBRE_COMPLETO        AS NOMBRE_PRODUCTOR,
    p.IDENTIFICACION         AS DOC_PRODUCTOR,
    p.DIRECCION              AS DIR_PRODUCTOR,
    p.TELEFONO               AS TEL_PRODUCTOR,
    p.CORREO_ELECTRONICO     AS CORREO_PRODUCTOR,
    p.DEPARTAMENTO_RESIDENCIA AS DEPTO_PRODUCTOR,

    l.ID_LOTE,
    l.NUMERO_LOTE,
    l.AREA_HECTAREAS,
    l.FECHA_SIEMBRA,
    l.FECHA_ELIMINACION,

    c.ID_CULTIVO,
    c.NOMBRE_COMUN           AS CULTIVO_NOMBRE_COMUN,
    c.NOMBRE_CIENTIFICO      AS CULTIVO_NOMBRE_CIENTIFICO,
    c.CICLO_CULTIVO
FROM ADMINICA.PRODUCTOR p
LEFT JOIN ADMINICA.LOTE l
       ON l.ID_PRODUCTOR = p.ID_PRODUCTOR
LEFT JOIN ADMINICA.CULTIVO c
       ON c.ID_CULTIVO = l.ID_CULTIVO
WHERE p.ID_PRODUCTOR = $P{P_ID_PRODUCTOR}
ORDER BY l.NUMERO_LOTE
    ]]></query>
