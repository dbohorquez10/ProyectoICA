<jasperReport name="usuarios" language="java" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="11111111-1111-1111-1111-111111111111">
	<parameter name="LOGO_ICA" class="java.lang.String"/>
	<query language="SQL"><![CDATA[SELECT
            TIPO,
            ID,
            IDENTIFICACION,
            NOMBRE_COMPLETO,
            DIRECCION,
            TELEFONO,
            CORREO
        FROM (
            SELECT
                'PRODUCTOR'              AS TIPO,
                p.ID_PRODUCTOR           AS ID,
                p.IDENTIFICACION         AS IDENTIFICACION,
                p.NOMBRE_COMPLETO        AS NOMBRE_COMPLETO,
                p.DIRECCION              AS DIRECCION,
                p.TELEFONO               AS TELEFONO,
                p.CORREO                 AS CORREO
            FROM ADMINICA.PRODUCTOR p

            UNION ALL

            SELECT
                'TECNICO'                AS TIPO,
                t.ID_TECNICO             AS ID,
                t.IDENTIFICACION         AS IDENTIFICACION,
                t.NOMBRE_COMPLETO        AS NOMBRE_COMPLETO,
                t.DIRECCION              AS DIRECCION,
                t.TELEFONO               AS TELEFONO,
                t.CORREO                 AS CORREO
            FROM ADMINICA.TECNICO t
        )
        ORDER BY TIPO, NOMBRE_COMPLETO]]></query>
