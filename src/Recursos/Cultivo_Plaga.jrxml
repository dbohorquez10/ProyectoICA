	<query language="sql"><![CDATA[
SELECT
    c.ID_CULTIVO,
    c.NOMBRE_COMUN       AS CULTIVO_NOMBRE_COMUN,
    c.NOMBRE_CIENTIFICO  AS CULTIVO_NOMBRE_CIENTIFICO,
    c.CICLO_CULTIVO,
    p.ID_PLAGA,
    p.NOMBRE_COMUN       AS PLAGA_NOMBRE_COMUN,
    p.NOMBRE_CIENTIFICO  AS PLAGA_NOMBRE_CIENTIFICO,
    p.TIPO_PLAGA
FROM ADMINICA.CULTIVO c
LEFT JOIN ADMINICA.CULTIVO_PLAGA cp
       ON cp.ID_CULTIVO = c.ID_CULTIVO
LEFT JOIN ADMINICA.PLAGA p
       ON p.ID_PLAGA = cp.ID_PLAGA
ORDER BY c.NOMBRE_COMUN, p.NOMBRE_COMUN
    ]]></query>
