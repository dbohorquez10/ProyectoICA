	<parameter name="P_ID_INSPECCION" class="java.lang.String"/>
	<parameter name="LOGO_ICA" class="java.lang.String"/>
	<query language="sql"><![CDATA[SELECT 
    i.ID_INSPECCION,
    i.FECHA_INSPECCION,
    i.TOTAL_PLANTAS,
    i.PLANTAS_AFECTADAS,
    i.ESTADO_FENOLOGICO,
    i.PORCENTAJE_INFESTACION,
    i.OBSERVACIONES,

    l.ID_LOTE,
    l.NUMERO_LOTE,
    l.AREA_HECTAREAS,
    l.FECHA_SIEMBRA,
    l.FECHA_ELIMINACION,
    l.ID_CULTIVO,

    t.ID_TECNICO,
    t.NOMBRE_COMPLETO AS NOMBRE_TECNICO,
    t.IDENTIFICACION AS DOC_TECNICO,
    t.TARJETA_PROFESIONAL,
    t.CORREO_ELECTRONICO AS CORREO_TECNICO,
    t.TELEFONO AS TEL_TECNICO,
    t.DEPARTAMENTO_RESIDENCIA AS DEPTO_TECNICO

FROM ADMINICA.INSPECCION i
JOIN ADMINICA.LOTE l ON l.ID_LOTE = i.ID_LOTE
JOIN ADMINICA.TECNICO t ON t.ID_TECNICO = i.ID_TECNICO
WHERE i.ID_INSPECCION = $P{P_ID_INSPECCION}]]></query>
